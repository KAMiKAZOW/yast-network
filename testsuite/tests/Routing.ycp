{

include "testsuite.ycp";

map READ = $[
    "target" : $[
	"size" : 1,
	"tmpdir" : "/tmp",
    ],
    "routes" : ["r1", "r2"],
    "sysconfig" : $[ "sysctl" : $[ "IP_FORWARD" : "yes" ]],
    "probe" : $[ "system" : [] ],
];

TESTSUITE_INIT([READ], nil);
import "Routing";

DUMP("Read");
TEST(``(Routing::Read()), [READ], nil);
DUMP(sformat("Routing::routes %1",Routing::routes));

DUMP("Write");
Routing::modified = true;
TEST(``(Routing::Write()), [READ], nil);
Routing::routes = [ "r1", "r3" ];
TEST(``(Routing::Write()), [READ], nil);

DUMP("Import");
TEST(``(Routing::Import($["routes":["r7", "r8"], "ip_forward":true])), [], nil);

DUMP("Export");
TEST(``(Routing::Export()), [], nil);

}
