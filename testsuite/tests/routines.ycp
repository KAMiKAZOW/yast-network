{

include "testsuite.ycp";

map READ = $[
    "probe" : $[
	"system" : [],
    ],
    "target" : $[
	"tmpdir" : "/tmp",
    ],
];

TESTSUITE_INIT([READ], nil);
include "network/routines.ycp";

DUMP("list2items");
TEST(``(list2items(["x","y"], 0)), [], nil);

DUMP("hwlist2items");
TEST(``(hwlist2items([$["name":"x"],$["name":"y"]], 0)), [], nil);

DUMP("device_type");
TEST(``(device_type("eth0")), [], nil);
