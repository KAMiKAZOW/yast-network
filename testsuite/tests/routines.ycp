TEST(``(device_type("eth-usb-1:0")), [], nil);
TEST(``(device_type("tr-pcmcia-2:432")), [], nil);
TEST(``(device_type("ippp2:2")), [], nil);
TEST(``(device_type("ppp2:2")), [], nil);
TEST(``(device_type("lo:1")), [], nil);
