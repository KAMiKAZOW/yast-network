Read	.probe.architecture nil
Read	.probe.has_apm nil
Read	.probe.has_pcmcia nil
Read	.probe.has_smp nil
Read	.probe.system []
Dump	ReadStatus
Return	true
Dump	RunSuSEconfig
Execute	.target.bash "/sbin/SuSEconfig --module hostname" 0
Execute	.target.bash "/sbin/SuSEconfig --module apache 2>/dev/null" 0
Execute	.target.bash "/sbin/SuSEconfig --module sendmail 2>/dev/null" 0
Execute	.target.bash "/sbin/SuSEconfig --module postfix 2>/dev/null" 0
Return	true
Execute	.target.bash "/sbin/SuSEconfig --module hostname" 1
Execute	.target.bash "/sbin/SuSEconfig --module apache 2>/dev/null" 1
Execute	.target.bash "/sbin/SuSEconfig --module sendmail 2>/dev/null" 1
Execute	.target.bash "/sbin/SuSEconfig --module postfix 2>/dev/null" 1
Return	true
Dump	StopNetwork
Execute	.target.bash "/etc/init.d/network stop" 0
Return	true
Execute	.target.bash "/etc/init.d/network stop" 1
Return	false
Dump	StartNetwork
Execute	.target.bash "/etc/init.d/network start" 0
Return	true
Execute	.target.bash "/etc/init.d/network start" 1
Return	false
Dump	RestartNetwork
Execute	.target.bash "/etc/init.d/network restart" 0
Return	true
Execute	.target.bash "/etc/init.d/network restart" 1
Return	false
Dump	SetupSMPPPD
Read	.init.scripts.exists "smpppd" true
Execute	.target.bash_output "/sbin/insserv -rf /etc/init.d/smpppd" $["exit":0, "stderr":"", "stdout":""]
Execute	.target.bash_output "/sbin/insserv /etc/init.d/smpppd,start=2,3,5" $["exit":0, "stderr":"", "stdout":""]
Read	.init.scripts.exists "smpppd" true
Execute	.target.bash "/etc/init.d/smpppd status" $["TERM":"raw"] 0
Execute	.target.bash "/etc/init.d/smpppd reload" $["TERM":"raw"] 0
Return	true
Read	.init.scripts.exists "smpppd" true
Execute	.target.bash_output "/sbin/insserv -rf /etc/init.d/smpppd" $["exit":1, "stderr":"Dummy error message", "stdout":""]
Log	Unable to disable service smpppd:
Read	.init.scripts.exists "smpppd" true
Execute	.target.bash "/etc/init.d/smpppd status" $["TERM":"raw"] 1
Execute	.target.bash "/etc/init.d/smpppd start" $["TERM":"raw"] 1
Return	true
