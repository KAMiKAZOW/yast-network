Dump	ReadStatus
Return	true
Dump	RunSuSEconfig
Execute	.target.bash "/sbin/SuSEconfig --quick --nonewpackage --module hostname" 0
Return	true
Execute	.target.bash "/sbin/SuSEconfig --quick --nonewpackage --module hostname" 1
Return	false
Dump	StopNetwork
Execute	.target.bash "/etc/init.d/network stop" 0
Return	true
Execute	.target.bash "/etc/init.d/network stop" 1
Return	false
Dump	StartNetwork
Execute	.target.bash "/etc/init.d/network start" 0
Return	true
Execute	.target.bash "/etc/init.d/network start" 1
Return	false
Dump	RestartNetwork
Execute	.target.bash "/etc/init.d/network restart" 0
Return	true
Execute	.target.bash "/etc/init.d/network restart" 1
Return	false
Dump	SetupSMPPPD
Read	.init.scripts.exists "smpppd" true
Execute	.target.bash "/sbin/insserv -r smpppd" 0
Execute	.target.bash "/sbin/insserv smpppd,start=2,3,5" 0
Read	.init.scripts.exists "smpppd" true
Execute	.target.bash "/etc/init.d/smpppd status" $["TERM":"raw"] 0
Execute	.target.bash "/etc/init.d/smpppd reload" $["TERM":"raw"] 0
Return	true
